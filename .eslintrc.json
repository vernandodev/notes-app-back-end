{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": "off",
        "no-console": "off",
        "consistent-return": "off"
    }
}
